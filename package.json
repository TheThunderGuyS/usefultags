{
  "name": "usefultags",
  "version": "1.2.0",
  "description": "A tiny JavaScript library providing some useful template tag functions.",
  "main": "dist/usefulTags.js",
  "browser": "dist/usefulTags.js",
  "unpkg": "dist/usefulTags.js",
  "module": "dist/usefulTags.mjs",
  "files": [
    "dist/*"
  ],
  "types": "dist/usefulTags.d.ts",
  "scripts": {
    "test": "node ./test.js && node ./test.mjs",
    "build": "npx babel ./usefulTags.mjs --out-file ./dist/usefulTags.js && npx minify ./usefulTags.mjs --sourceType module --out-file ./dist/usefulTags.mjs && npx ncp ./usefulTags.d.ts ./dist/usefulTags.d.ts"
  },
  "engines": {
    "node": ">=0.2",
    "iojs": "*",
    "deno": "*"
  },
  "browserslist": [
    "firefox 4",
    "chrome 19",
    "ie 9",
    "node 0.2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheThunderGuyS/usefulTags.git"
  },
  "keywords": [
    "es6",
    "es2015",
    "tagged-template-literals",
    "template-literals",
    "template-string",
    "template-strings",
    "indentation"
  ],
  "author": "TheThunderGuyS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheThunderGuyS/usefulTags/issues"
  },
  "homepage": "https://github.com/TheThunderGuyS/usefulTags#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-eslint": "^10.1.0",
    "babel-minify": "^0.5.1",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ncp": "^2.0.0",
    "prettier": "^2.2.1"
  }
}
